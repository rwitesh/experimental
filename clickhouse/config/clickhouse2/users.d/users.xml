<clickhouse>
    <profiles>
        <default>
            <date_time_input_format>best_effort</date_time_input_format>
            <allow_experimental_time_series_aggregate_functions>1</allow_experimental_time_series_aggregate_functions>
        </default>
    </profiles>

    <users>
        <default>
           <allow_no_password>1</allow_no_password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>
    </users>
</clickhouse>
